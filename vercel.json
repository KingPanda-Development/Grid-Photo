{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "secretfile/index.html", "use": "@vercel/static" },
    { "src": "downloadlist/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "^/secretfile$",
      "dest": "/secretfile/index.html"
    },
    {
      "src": "^/secretfile/(.*)",
      "dest": "/secretfile/$1"
    },
    {
      "src": "^/downloadlist$",
      "dest": "/downloadlist/index.html"
    },
    {
      "src": "^/downloadlist/(.*)",
      "dest": "/downloadlist/$1"
    },
    {
      "src": "^(.*)$",
      "dest": "/index.html"
    }
  ]
}
