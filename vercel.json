{
  "version": 2,
  "name": "grid-photo",
  "builds": [
    { "src": "**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "^/secretfile$",
      "dest": "/secretfile/index.html",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8"
      }
    },
    {
      "src": "^/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/index.html"
    }
  ]
}
